     StackState = struct empty | full ;
	  door = struct DI1 | DI2 | DO1 | DO2 ;

act MoveTo: Destination;
	 PickupWafer;
	 PlaceWafer;
	openDoor : door;
         closeDoor : door;

	 


proc

% IO handler 1

GetProcess1 = (MoveTo(A1) + MoveTo(I1)). PickupWafer. PutProcess1;
